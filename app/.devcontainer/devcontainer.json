{
  "name": "CDN Express Dev APP",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "cdn",
  "forwardPorts": [
    3000
  ],
  "workspaceFolder": "/app-workspace",
  "settings": {
    "editor.formatOnSave": true,
    "terminal.integrated.defaultProfile.linux": "usr/bin/bash",
    "terminal.integrated.profiles.linux": {
      "bash": {
        "path": "/bin/bash"
      },
      "zsh": {
        "path": "/bin/zsh"
      },
      "sh": {
        "path": "/bin/sh"
      }
    }
  },
  "extensions": [
    "ms-azuretools.vscode-docker",
    "eamodio.gitlens"
  ],
  "remoteUser": "node"
}